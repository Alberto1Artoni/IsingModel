Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"Glauber", " ", "update"}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"b\[Sigma]", "=", 
     RowBox[{"RandomChoice", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"1", ",", 
         RowBox[{"-", "1"}]}], "}"}], ",", "Nn"}], "]"}]}], ";"}], 
   RowBox[{"(*", 
    RowBox[{"random", " ", "initial", " ", "state"}], "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{"For", "[", 
    RowBox[{
     RowBox[{"iT", " ", "=", " ", "1"}], ",", " ", 
     RowBox[{"iT", " ", "<=", "NkT"}], ",", " ", 
     RowBox[{"iT", "++"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"\[Beta]", "=", 
       RowBox[{"1", "/", 
        RowBox[{"kT", "[", 
         RowBox[{"[", "iT", "]"}], "]"}]}]}], ";", 
      RowBox[{"(*", 
       RowBox[{"\[Beta]", "=", 
        RowBox[{"1", "/", "kT"}]}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"For", "[", 
       RowBox[{
        RowBox[{"kk", "=", "2"}], ",", 
        RowBox[{"kk", "\[LessEqual]", 
         RowBox[{"mcs", "+", "1"}]}], ",", 
        RowBox[{"kk", "++"}], ",", "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{"Monitor", "["}], "*)"}], "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"For", "[", 
          RowBox[{
           RowBox[{"t", "=", "1"}], ",", 
           RowBox[{"t", "\[LessEqual]", "Nn"}], ",", 
           RowBox[{"t", "++"}], ",", "\[IndentingNewLine]", 
           RowBox[{"(*", 
            RowBox[{
            "loop", " ", "over", " ", "Monte", " ", "Carlo", " ", "steps"}], 
            "*)"}], 
           RowBox[{"(*", 
            RowBox[{
            "choose", " ", "the", " ", "spin", " ", "to", " ", "flip"}], 
            "*)"}], "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"xflip", "=", 
             RowBox[{"RandomInteger", "[", 
              RowBox[{"{", 
               RowBox[{"1", ",", "Nn"}], "}"}], "]"}]}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{"(*", 
             RowBox[{"acceptance", " ", "step"}], "*)"}], 
            "\[IndentingNewLine]", 
            RowBox[{"flipped", " ", "=", " ", "b\[Sigma]"}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"flipped", "[", 
              RowBox[{"[", "xflip", "]"}], "]"}], " ", "=", " ", 
             RowBox[{"-", 
              RowBox[{"flipped", "[", 
               RowBox[{"[", "xflip", "]"}], "]"}]}]}], " ", ";", 
            "\[IndentingNewLine]", 
            RowBox[{"myH", "[", 
             RowBox[{"flipped", ",", "myA"}], "]"}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{"deltaH", " ", "=", " ", 
             RowBox[{"(", 
              RowBox[{"h", " ", "-", " ", "hOld"}], ")"}]}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{"\[Alpha]", "=", 
             RowBox[{"Min", "[", 
              RowBox[{"1", ",", 
               RowBox[{"1", "/", 
                RowBox[{"(", 
                 RowBox[{"1", "+", 
                  RowBox[{"Exp", "[", 
                   RowBox[{"2", "*", "\[Beta]", " ", "deltaH"}], "]"}]}], 
                 ")"}]}]}], "]"}]}], ";", "\[IndentingNewLine]", 
            RowBox[{"\[Alpha]", "=", 
             RowBox[{"Min", "[", 
              RowBox[{"1", ",", 
               RowBox[{"Exp", "[", 
                RowBox[{
                 RowBox[{"-", "\[Beta]"}], " ", 
                 RowBox[{"(", "deltaH", ")"}]}], "]"}]}], "]"}]}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{"u", "=", 
             RowBox[{"RandomReal", "[", 
              RowBox[{"{", 
               RowBox[{"0", ",", "1"}], "}"}], "]"}]}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{"u", "\[LessEqual]", "\[Alpha]"}], ",", 
              RowBox[{
               RowBox[{"tt", "=", 
                RowBox[{"b\[Sigma]", "[", 
                 RowBox[{"[", "xflip", "]"}], "]"}]}], ";", 
               "\[IndentingNewLine]", 
               RowBox[{"hOld", " ", "=", " ", "h"}], ";", 
               "\[IndentingNewLine]", 
               RowBox[{"h", " ", "=", " ", "0"}], ";", "\[IndentingNewLine]", 
               
               RowBox[{
                RowBox[{"b\[Sigma]", "[", 
                 RowBox[{"[", "xflip", "]"}], "]"}], "=", 
                RowBox[{"-", "tt"}]}]}], ",", 
              RowBox[{
               RowBox[{"h", " ", "=", " ", "0"}], ";"}]}], 
             "\[IndentingNewLine]", "]"}], ";"}]}], "]"}], ";", 
         RowBox[{"(*", 
          RowBox[{"end", " ", "For", " ", "i"}], "*)"}], 
         RowBox[{"(*", 
          RowBox[{"calculate", "<", "M", ">", "and", "<", 
           RowBox[{"M", "^", "2"}], ">"}], "*)"}], "\[IndentingNewLine]", 
         RowBox[{"(*", 
          RowBox[{
           RowBox[{"plot", "=", 
            RowBox[{"ArrayPlot", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{
                RowBox[{"1", "/", "2"}], " ", 
                RowBox[{"(", 
                 RowBox[{"b\[Sigma]", "+", "1"}], ")"}]}], "}"}], ",", 
              RowBox[{"Mesh", "\[Rule]", "All"}]}], "]"}]}], ";"}], "*)"}], 
         "\[IndentingNewLine]", 
         RowBox[{"Mt", " ", "=", " ", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"Mt0", 
              RowBox[{"(", 
               RowBox[{"kk", "-", "1"}], ")"}]}], "+", 
             RowBox[{"Total", "[", "b\[Sigma]", "]"}]}], ")"}], "/", "kk"}]}],
          ";", " ", 
         RowBox[{"Mt0", " ", "=", " ", "Mt"}], ";", "\[IndentingNewLine]", 
         " ", 
         RowBox[{"M2t", " ", "=", " ", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"M2t0", 
              RowBox[{"(", 
               RowBox[{"kk", "-", "1"}], ")"}]}], "+", 
             RowBox[{
              RowBox[{"Total", "[", "b\[Sigma]", "]"}], "^", "2"}]}], ")"}], 
           "/", "kk"}]}], ";", " ", 
         RowBox[{"M2t0", " ", "=", " ", "M2t"}], ";"}]}], " ", 
       "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{
         RowBox[{"Print", "[", 
          RowBox[{
          "\"\<End of the \>\"", ",", "kk", ",", "\"\< mcs iteration\>\""}], 
          "]"}], ";"}], "*)"}], "\[IndentingNewLine]", "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Mtvett", "[", 
        RowBox[{"[", "iT", "]"}], "]"}], " ", "=", " ", "Mt"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"\[Chi]T", "[", 
        RowBox[{"[", "iT", "]"}], "]"}], " ", "=", " ", 
       RowBox[{"\[Beta]", 
        RowBox[{"(", 
         RowBox[{"M2t", " ", "-", " ", 
          RowBox[{"Mt", "^", "2"}]}], ")"}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Print", "[", 
       RowBox[{
       "\"\<End of the \>\"", ",", "iT", ",", "\"\< full iteration\>\""}], 
       "]"}], ";"}]}], "\[IndentingNewLine]", "]"}], "\[IndentingNewLine]", 
   RowBox[{"(*", 
    RowBox[{
     RowBox[{
      RowBox[{",", "plot"}], "]"}], ";"}], "*)"}], "\[IndentingNewLine]", 
   RowBox[{"(*", 
